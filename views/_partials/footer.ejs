    <div class="footer-select">
        <% if (false/*!bs_host && !sc_host && !asic*/) { %>
            <select class="branch" name="" title="<%-$t('regulator_selector_tooltip', true)%>">
                <%
                    let branchNames = {
                        '1': 'asic',
                        '2': 'cysec',
                        '3': 'fsa',
                        '4': 'scb',
                        '6': 'fca',
                        '8': 'fsc',
                        '9': 'cima'
                    };
                    let branchState = {
                        asic, cysec, fsa, scb
                    };
                    let hasSelected = false;
                    function getRegulatorName (branch) {
                        let names = {
                            asic: 'AU',
                            cysec: 'EU',
                            fsa: 'GLOBAL',
                            scb: 'INTL',
                            fca: 'UK',
                            fsc: 'MU',
                            cima: 'KY'
                        };
                        return names[branch];
                    }
                    branching && branching.filter(function (b) {
                        if (cysec) {
                            if (b.branch != '2' && b.branch != '3') return false;
                        } else {
                            if (b.branch === '2') return false;
                        }
                        return b.status !== '4';
                    }).forEach(function (b) {
                        var bb = branchNames[b.branch];
                        if (!hasSelected) {
                            hasSelected = !!branchState[bb];
                        } %>
                        <option <%= branchState[bb] ? 'selected="selected"' : '' %> value="<%= bb %>"><%= getRegulatorName(bb) %></option>
                    <% });
                    if (!hasSelected) { %>
                        <option selected="selected" disabled value="<%= branchNames[branchID] %>"><%= getRegulatorName(branchNames[branchID]) %></option>
                <% } %>
            </select>
        <% } %>
