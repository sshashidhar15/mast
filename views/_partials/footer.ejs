                                Cookies.remove('reg_force_country');
                                let camp = Cookies.get('camp');
                                let href = `https://www.icmarkets.eu${camp && camp.length ? '/?camp=' + camp : ''}`;
                                window.location.href = href;
                            }
                        });
                    }
                });
            }, 2000);
        </script>
    <% } %>
    <% if (countryCode == 'au' && fsa) { %>
        <script>
            setTimeout(function () {
                $(document).ready(function() {
                    var auOutsideCookieName = 'au_user_accepted_not_asic_branch';
                    var auOutsideClassName = 'au-user-accepted-not-asic-branch';
                    var auOutsideAlreadySetCookie = Cookies.get(auOutsideCookieName);
                    var isAustralia = '<%= (countryCode == "au" ? 1 : 0) %>';
                    if (isAustralia === '1' && auOutsideAlreadySetCookie != ICMBranchID) {
                        $('body').toggleClass('popup-special-active-au-outside', true);
                        $('.popup-special.au-outside .content').off('click', '.popup-special-button');
                        $('.popup-special.au-outside .content').on('click', '.popup-special-button', function (e) {
                            $('body').toggleClass('popup-special-active-au-outside', false);
                            var btn = $(this);
                            if (btn.is('.accept-button')) {
                                Cookies.set(auOutsideCookieName, ICMBranchID, { expires: 365 });
                            } else {
                                Cookies.remove('reg_force_country');
                                let camp = Cookies.get('camp');
                                let href = `https://www.icmarkets.com/au/en/${camp && camp.length ? '/?camp=' + camp : ''}`;
                                window.location.href = href;
                            }
                        });
                    }
                });
            }, 2000);
        </script>
    <% } %>
    <script>
        function toggleBranch(regulator) {
            if (ICMBranchID === '2') {
                if (regulator === 'asic') {
                    $('body').toggleClass('popup-modal-active-change-to-au', true);
                    $('.popup-modal.change-to-au .content').off('click', '.popup-modal-button');
                    $('.popup-modal.change-to-au .content').on('click', '.popup-modal-button', function (e) {
                        $('body').toggleClass('popup-modal-active-change-to-au', false);
                        var btn = $(this);
                        if (btn.is('.accept-button')) {
                            // redirect to ASIC website after accept only
                            // and to the similar website page
                            Cookies.remove('regulator');
                            window.location = window.location.origin.replace('.eu', '.com') + '/?regulator=1&rfeu=1';
                        } else { // restore CySEC in selector
                            // leave on the CySEC website
                            $('.footer-select select').val('cysec');
                        }
                    });
                } else if (regulator === 'fsa') {
                    $('body').toggleClass('popup-modal-active-change-to-sc', true);
                    $('.popup-modal.change-to-sc .content').off('click', '.popup-modal-button');
                    $('.popup-modal.change-to-sc .content').on('click', '.popup-modal-button', function (e) {
                        $('body').toggleClass('popup-modal-active-change-to-sc', false);
                        var btn = $(this);
                        if (btn.is('.accept-button')) {
                            // redirect to FSA website after accept only
                            // and to the similar website page
                            Cookies.remove('regulator');
                            window.location = window.location.origin.replace('.eu', '.com') + '/?regulator=3&rfeu=1';
                        } else { // restore CySEC in selector
                            // leave on the CySEC website
                            $('.footer-select select').val('cysec');
                        }
                    });
                } else if (regulator === 'scb') {
                    $('body').toggleClass('popup-modal-active-change-to-bs', true);
                    $('.popup-modal.change-to-bs .content').off('click', '.popup-modal-button');
                    $('.popup-modal.change-to-bs .content').on('click', '.popup-modal-button', function (e) {
                        $('body').toggleClass('popup-modal-active-change-to-bs', false);
                        var btn = $(this);
                        if (btn.is('.accept-button')) {
                            // redirect to SCB website after accept only
                            // and to the similar website page
                            Cookies.remove('regulator');
                            window.location = window.location.origin.replace('.eu', '.com') + '/?regulator=4&rfeu=1';
                        } else { // restore CySEC in selector
                            // leave on the CySEC website
                            $('.footer-select select').val('cysec');
                        }
                    });
                }
            } else {
                if (regulator === 'asic') {
                    Cookies.set('regulator', '1');
                } else if (regulator === 'cysec') {
                    Cookies.remove('regulator');
                    window.location = window.location.origin.replace('.com', '.eu');
                } else if (regulator === 'scb') {
                    Cookies.set('regulator', '4');
                } else /*if (regulator === 'fsa')*/ {
                    Cookies.set('regulator', '3');
                }
                window.location.reload(true);
            }
        }
        function toggleCountryFrom(code) {
            Cookies.set('country_from', code);
            Cookies.remove('regulator');
            window.location.reload(true);
        }
        setTimeout(function () {
            $(document).ready(function() {
                $('.footer-select select.branch').on('change', function (e) {
                    toggleBranch($(this).val());
                });
                $('.footer-select select.country_from').on('change', function (e) {
                    toggleCountryFrom($(this).val());
                });
            });
        }, 2000);
    </script>
</footer>
