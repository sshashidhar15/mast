            if (translator_should_load_his_translations_from_pendings) {
                fetch(
                    location.origin + '/<%- prefix_by_regulator %>/<%- locale %>/translator/translations_from_pending',
                    {
                        method: 'POST',
                        headers: {
                            "Content-Type": "application/json"
