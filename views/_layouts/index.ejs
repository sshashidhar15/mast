                    .then(_ => _.json())
                    .then(translations => {
                        Object.keys(translations).forEach(lang =>
                            localStorage.setItem('translations-' + lang, JSON.stringify(translations[lang]))
                        )
                        Cookies.remove('translator_should_load_his_translations_from_pendings')
                    })
            }
