                        <div class="flex-content">
                            <div class="content-head"><%-$t('indices_28')%></div>
                            <div class="content-inside ordinary"><%-$t('indices_29')%></div>
                            <div class="content-inside description"><%-$t('indices_30')%></div>
                        </div>
                        <div class="flex-content">
                            <div class="content-head"><%-$t('indices_31')%></div>
                            <div class="content-inside ordinary"><%-$t('indices_32')%></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="page-content highlighted-content">
                <div class="page-section split-2">
                    <div class="left-section">
                        <h1 style="padding-right:30px;"><%-$t('indices_33')%></h1>
                        <div class="risk-image"></div>
                    </div>
                    <div class="right-section">
                        <p><%-$t('indices_34')%></p>
                        <p><%-$t('indices_35')%></p>
                    </div>
                </div>
            </div>
            <div class="page-content">
                <div class="page-section">
                    <h1><%-$t('indices_36')%></h1>
                    <div class="accent-block"><%-$t('indices_37')%></div>
                    <article class="spreads-block icm-page-subsection-content">
                        <div class="spreads-block__spread-item">
                            <div class="spreads-accordion">
                                <div class="panel-collapse collapse in">
                                    <%- include('../_partials/components/indices-spread-table.ejs') %>
                                </div>
                            </div>
                        </div>
                    </article>
                </div>
            </div>
            <% if (asic) { %>
            <div class="page-content">
                <div class="page-section">
                    <div class="profile-buttons">
                        <a id="indices_3" style="text-decoration:none;" class="profile-button" href="<%= localePath('trading-pricing-spreads')%>">
                            <div class="profile-button-label"><%-$t('indices_381')%></div>
                            <div class="profile-button-value"><%-$t('indices_382')%></div>
                        </a>
                        <a id="indices_4" style="text-decoration:none;" class="profile-button" href="<%- getCDN() %>/uploads/AU/Indices-Product-Specification-Sheet.pdf" target="_blank">
                            <div class="profile-button-label"><%-$t('indices_391')%></div>
                            <div class="profile-button-value"><%-$t('indices_392')%></div>
                        </a>
                        <a id="indices_5" style="text-decoration:none;" class="profile-button" href="<%= localePath('trading-accounts-raw-spread-account')%>">
                            <div class="profile-button-label"><%-$t('indices_401')%></div>
                            <div class="profile-button-value"><%-$t('indices_4021')%></div>
                        </a>
                    </div>
                </div>
            </div>
            <% } %>
            <div class="page-content global-markets-today">
                <div class="page-section">
                    <div class="start-trading"><%-$t('indices_41')%></div>
                    <div class="global-today"><%-$t('indices_42')%></div>
                    <div class="icm-button-wrapper page-header__primary-button">
                        <a id="indices_6" class="icm-btn live icm-btn-primary" href="<%= localePath('open-trading-account-live')%>"><%-$t('indices_43')%></a>
                        <a id="indices_7" class="icm-btn demo" href="<%= localePath('open-trading-account-demo')%>"><%-$t('indices_44')%></a>
                    </div>
                </div>
            </div>
        </article>
    </div>
